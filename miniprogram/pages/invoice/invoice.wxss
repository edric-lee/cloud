page {
  display: block;
  /* min-height: 100%; */
  background: linear-gradient(#2193b0, #2193b0);
}

.cro {
  width: 90%;
  /* border: 1px solid #000; */
  position: relative;
  margin: 30px auto;
  background-color: white;
}

.head {
  height: 120px;
  border-bottom: 1px dashed #2193b0;
}

.logo {
  height: 100%;
  width: 100%;
  /* border: 1px dashed #2193b0; */
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
} 

.logo image {
  width: 60%;
}

