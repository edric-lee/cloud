.body {
  margin-top: 90rpx;
}

.content {
  width: 90%;
  height: 80rpx;
  /* border: 1px solid #000; */
  margin: 0 auto;
  display: flex;
  line-height: 20px;
  /* flex-wrap:nowrap; */
  font-size: 30rpx;
}

.text_left {
  width: 35%;
  /* border: 1px solid #000; */
}

.text_right {
  width: 70%;
  /* border: 1px solid #000; */
}

.warning {
  width: 90%;
  color: red;
  margin: 60px auto;
}

.cro_left_top, .cro_right_top, .cro_left_bottom, .cro_right_bottom {
  position: absolute;
  width: 10px;
  height: 10px;
  border: 1px solid #2193b0;
  z-index: 1;
  background: #2193b0;
}

.cro_left_top {
  top: 120px;
  left: -1px;
  border-radius: 0px 0px 10px 0px;
  /* border-bottom: 1px solid #000; *//* border-right: 1px solid #000; */
}

.cro_right_top {
  top: 120px;
  right: -1px;
  border-radius: 0px 0px 0px 10px;
  /* border-bottom: 1px solid #000;
  border-left: 1px solid #000; */
}

.cro_left_bottom {
  left: -1px;
  top: 110px;
  border-radius: 0px 10px 0px 0px;
  /* border-top: 1px solid #000;
  border-right: 1px solid #000; */
}

.cro_right_bottom {
  right: -1px;
  top: 110px;
  border-radius: 10px 0px 0px 0px;
  /* border-top: 1px solid #000;
  border-left: 1px solid #000; */
}
